{
  "name": "webrtc-video-room",
  "version": "2.0.3",
  "description": "WebRTC Video Room is a Single-Page App in React using React Router that provides peer-to-peer video and audio communication in a web browser with minimum dependencies required.",
  "scripts": {
    "start": "node --openssl-legacy-provider ./node_modules/.bin/webpack build && node server.js",
    "serverstart": "npx webpack && node server.js",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "postinstall": "webpack --mode production",
    "watch": "webpack --watch"
  },
  "keywords": [
    "WebRTC Video Chat",
    "RTCPeerConnection",
    "React"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/runtime": "^7.12.18",
    "@material-ui/core": "^4.11.3",
    "@mediapipe/camera_utils": "^0.3.1640029074",
    "@mediapipe/drawing_utils": "^0.3.1620248257",
    "@mediapipe/face_detection": "^0.4.1628005423",
    "@mediapipe/face_mesh": "^0.4.1633559619",
    "compression": "^1.7.2",
    "dotenv": "^14.2.0",
    "express": "^4.16.3",
    "face-api.js": "^0.22.2",
